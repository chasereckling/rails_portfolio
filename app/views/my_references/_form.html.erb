<%= render "layouts/errors", :object => @my_reference %>

<%= form_for @my_reference, remote: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :title %>
  <%= f.text_area :title %>
  <%= f.label :phone %>
  <%= f.text_area :phone %>
  <%= f.label :address %>
  <%= f.text_area :address %>
  <%= f.label :relationship %>
  <%= f.text_area :relationship %>
  <%= f.submit %>
<% end %>
